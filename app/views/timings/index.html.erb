<div class="page-header">
  <h1>
    Timings
    <a href="/timings/new" class="btn btn-lg btn-success">New Timing</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Time</th>
        <th>Date</th>
        <th>Day</th>
        <th>Reminders</th>
        <th>Actions</th>
      </tr>

      <% @timings.each do |timing| %>
      <tr>
        <td><%= timing.time %></td>
        <td><%= timing.date %></td>
        <td><%= timing.day %></td>
        <td><%= timing.reminders.focus_id %></td>
        <td>
          <a href="/timings/<%= timing.id %>" class="btn btn-primary">Show</a>
          <a href="/timings/<%= timing.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_timing/<%= timing.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
